# App Configuration
VITE_APP_NAME=TikTok App
VITE_API_URL=http://localhost:1420

# Features
VITE_ENABLE_EMOJI_OVERLAYS=true
VITE_ENABLE_SCENE_DETECTION=true

# Video Processing
VITE_MAX_VIDEO_SIZE=1024 # MB
VITE_SUPPORTED_FORMATS=mp4,mov,webm

# Paths (for development)
VITE_FFMPEG_PATH=/usr/local/bin/ffmpeg
VITE_PYTHON_PATH=/usr/bin/python3

# Optional: Analytics and Error Reporting
# VITE_SENTRY_DSN=your-sentry-dsn
# VITE_GA_TRACKING_ID=your-ga-id